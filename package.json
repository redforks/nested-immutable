{
  "name": "nested-immutable",
  "version": "1.0.0",
  "description": "Nested immutable structure",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "tsc --noEmit -p . && yarn tslint -p .",
    "clean": "rm -rf lib",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "repository": "https://github.com/redforks/nested-immutable",
  "author": "redforks",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/ramda": "^0.25.47",
    "mytslint": "^1.3.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.14.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "funts": "^1.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "peerDependencies": {
    "ramda": "^0.26.0",
    "ramda-adjunct": "^2.14.0",
    "tslib": "^1.9.3"
  }
}
